    border: none;
    font-weight: bold;
    border-radius: 5px;
    color: white;
    text-align: center;
}

/* Button Hover Effect */
QPushButton:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #66BFFF, stop:1 #80D5FF);
}

/* Button Pressed Effect */
QPushButton:pressed {
    background-color: rgba(255, 255, 255, 0.2);
}

/* LineEdit Style */
QLineEdit {
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    padding: 4px;
	color: rgb(0, 0, 0);
    background-color: #FFFFFF;
}

QLineEdit:focus {
    border: 1px solid #007AFF;
    background-color: #F0F8FF;
}

/* ComboBox Style */
QComboBox {
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    padding: 4px;
	color: black;
    background-color: #FFFFFF;
}

QComboBox:hover {
    border: 1px solid #AAAAAA;
}

QComboBox:focus {
    border: 1px solid #007AFF;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #CCCCCC;
}

QComboBox::down-arrow {
    image: url(src/asset/images/arrow-down.png);  /* You'll need to provide this image or use a different styling */
